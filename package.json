{
  "name": "@admiral-ds/react-ui",
  "version": "5.4.0",
  "description": "React component library for Admiral design system 2.1",
  "keywords": [
    "AdmiralDS",
    "Admiral",
    "ui",
    "react",
    "library"
  ],
  "author": "Admiral DS team",
  "license": "See license in ./LICENSE",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist",
    "src",
    "tsconfig.json"
  ],
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "./dist/src/index.d.ts",
  "type": "module",
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "prepare": "npm run build",
    "postinstall": "npm run build",
    "build": "rollup -c",
    "test": "jest",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --quiet --fix",
    "storybook": "npm run generate-svg-icon-exports && storybook dev -p 6006",
    "build-storybook": "npm run generate-svg-icon-exports && storybook build",
    "release": "standard-version",
    "generate-svg-icon-exports": "node scripts/generate-svg-icon-exports.cjs",
    "prepublishOnly": "npm run build",
    "deploy-storybook": "NODE_OPTIONS=--openssl-legacy-provider storybook-to-ghpages"
  },
  "dependencies": {
    "@admiral-ds/flags": "^2.0.0",
    "@admiral-ds/fonts": "^2.0.0",
    "@admiral-ds/icons": "^2.0.0",
    "dayjs": "^1.11.7",
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": "^5.1.0"
  },
  "peerDependencies": {
    "@admiral-ds/flags": "^2.0.0",
    "@admiral-ds/fonts": "^2.0.0",
    "@admiral-ds/icons": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@admiral-ds/react-ui": "^5.1.0",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/plugin-url": "^8.0.1",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/react-webpack5": "^7.0.8",
    "@storybook/testing-library": "^0.2.0",
    "@svgr/rollup": "^8.0.1",
    "@svgr/webpack": "^8.0.1",
    "@tanstack/react-query": "^4.29.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^12.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.12",
    "@types/react": "^17.0.0",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@types/styled-components": "^5.1.26",
    "file-loader": "^6.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.5.0",
    "jest-styled-components": "^7.1.1",
    "react-router-dom": "^6.13.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^3.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "storybook": "^7.0.8",
    "ts-jest": "^29.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.0"
  },
  "repository": "https://github.com/AdmiralDS/react-ui",
  "bugs": {
    "url": "https://github.com/AdmiralDS/react-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
