{
  "name": "@admiral-ds/react-ui",
  "version": "8.4.7",
  "description": "React component library for Admiral design system 2.1",
  "keywords": [
    "AdmiralDS",
    "Admiral",
    "ui",
    "react",
    "library"
  ],
  "author": "Admiral DS team",
  "license": "See license in ./LICENSE",
  "files": [
    "dist",
    "src/components",
    "README.md",
    "tsconfig.json"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": false,
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c --strictDeprecations",
    "test": "jest --detectOpenHandles",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build --disable-telemetry --output-dir docs",
    "preview-storybook": "npx http-server ./docs",
    "release": "standard-version",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@admiral-ds/fonts": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5.1.0"
  },
  "devDependencies": {
    "@admiral-ds/flags": "^3.1.1",
    "@admiral-ds/fonts": "^2.0.0",
    "@admiral-ds/icons": "^4.3.1",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@rollup/plugin-commonjs": "^25.0.8",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-url": "^8.0.2",
    "@storybook/addon-designs": "^7.0.9",
    "@storybook/addon-essentials": "^7.6.19",
    "@storybook/addon-interactions": "^7.6.19",
    "@storybook/addon-links": "^7.6.19",
    "@storybook/addon-styling": "^1.3.7",
    "@storybook/blocks": "^7.6.19",
    "@storybook/react": "^7.6.19",
    "@storybook/react-webpack5": "^7.6.19",
    "@storybook/testing-library": "^0.2.2",
    "@styled/typescript-styled-plugin": "^1.0.1",
    "@svgr/rollup": "^8.1.0",
    "@svgr/webpack": "^8.1.0",
    "@tanstack/react-query": "^5.45.1",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.7",
    "@types/react": "^18.3.3",
    "@types/rollup-plugin-peer-deps-external": "^2.2.5",
    "@types/serve-static": "^1.15.7",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "babel-plugin-styled-components": "^2.1.4",
    "dayjs": "^1.11.11",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "file-loader": "^6.2.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-styled-components": "^7.2.0",
    "prettier": "^3.3.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.23.1",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^4.18.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-tsc-alias": "^1.1.2",
    "standard-version": "^9.5.0",
    "storybook": "^7.6.19",
    "storybook-addon-dir": "^1.0.6",
    "storybook-dark-mode": "4.0.1",
    "styled-components": "^6.1.11",
    "ts-jest": "^29.1.5",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "^5.5.2",
    "typescript-plugin-styled-components": "^3.0.0"
  },
  "repository": "https://github.com/AdmiralDS/react-ui",
  "bugs": {
    "url": "https://github.com/AdmiralDS/react-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
