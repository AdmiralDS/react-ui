{
  "name": "@admiral-ds/react-ui",
  "version": "6.1.0",
  "description": "React component library for Admiral design system 2.1",
  "keywords": [
    "AdmiralDS",
    "Admiral",
    "ui",
    "react",
    "library"
  ],
  "author": "Admiral DS team",
  "license": "See license in ./LICENSE",
  "files": [
    "dist",
    "src/components",
    "README.md",
    "tsconfig.json"
  ],
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs.js",
  "module": "dist/index.es.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.es.js"
    }
  },
  "sideEffects": false,
  "imports": {
    "#src/*": "./src/*"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "rollup -c",
    "test": "jest --detectOpenHandles",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "storybook": "npm run generate-svg-icon-exports && storybook dev -p 6006",
    "build-storybook": "npm run generate-svg-icon-exports && storybook build --disable-telemetry --output-dir docs",
    "preview-storybook": "npx http-server ./docs",
    "release": "standard-version",
    "generate-svg-icon-exports": "node scripts/generate-svg-icon-exports.cjs",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@admiral-ds/fonts": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "peerDependencies": {
    "@admiral-ds/fonts": "^2.0.0",
    "react": ">=16",
    "react-dom": ">=16",
    "styled-components": ">=5.1.0"
  },
  "devDependencies": {
    "@admiral-ds/flags": "^3.0.0",
    "@admiral-ds/icons": "^3.0.1",
    "@babel/preset-env": "^7.21.5",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.5",
    "@rollup/plugin-commonjs": "^25.0.1",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.0",
    "@rollup/plugin-url": "^8.0.1",
    "@storybook/addon-designs": "^7.0.0",
    "@storybook/addon-essentials": "^7.0.8",
    "@storybook/addon-interactions": "^7.0.8",
    "@storybook/addon-links": "^7.0.8",
    "@storybook/addon-styling": "^1.0.8",
    "@storybook/blocks": "^7.0.8",
    "@storybook/react": "^7.0.8",
    "@storybook/react-webpack5": "^7.0.8",
    "@storybook/testing-library": "^0.2.0",
    "@styled/typescript-styled-plugin": "^1.0.0",
    "@svgr/rollup": "^8.0.1",
    "@svgr/webpack": "^8.0.1",
    "@tanstack/react-query": "^4.29.5",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.12",
    "@types/react": "^18.0.0",
    "@types/rollup-plugin-peer-deps-external": "^2.2.1",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "babel-plugin-styled-components": "^2.1.4",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^5.0.0",
    "file-loader": "^6.2.0",
    "jest": "^29.5.0",
    "jest-environment-jsdom": "^29.6.1",
    "jest-styled-components": "^7.1.1",
    "prettier": "^3.0.0",
    "react-router-dom": "^6.13.0",
    "resize-observer-polyfill": "^1.5.1",
    "rollup": "^3.0.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-tsc-alias": "^1.1.2",
    "standard-version": "^9.5.0",
    "storybook": "^7.0.8",
    "storybook-addon-rtl": "^0.5.0",
    "storybook-dark-mode": "^3.0.0",
    "styled-components": "^5.1.0",
    "ts-jest": "^29.1.0",
    "tsconfig-paths-webpack-plugin": "^4.0.1",
    "typescript": "^5.1.6",
    "typescript-plugin-styled-components": "^3.0.0"
  },
  "repository": "https://github.com/AdmiralDS/react-ui",
  "bugs": {
    "url": "https://github.com/AdmiralDS/react-ui/issues"
  },
  "publishConfig": {
    "access": "public"
  }
}
